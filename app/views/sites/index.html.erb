<% title "Dashboard" %>

<div class="row">
<div class="eightcol">
<iframe src="https://www.google.com/calendar/embed?showTitle=0&amp;showPrint=0&amp;height=500&amp;wkst=1&amp;bgcolor=%23FFFFFF&amp;src=thogg4%40gmail.com&amp;color=%23060D5E&amp;ctz=America%2FDenver" style=" border-width:0 " width="700" height="500" frameborder="0" scrolling="no"></iframe>
</div>
<div class="fourcol last">
	<% @sites.each do |site| %>
		<div class="title2">
			Tasks for <%= site.name %> <%= link_to image_tag("delete.png"), site_path(site), :method => :delete, :confirm => "Are you sure? This will delete everything!!" %>
		</div>
			<% site.tasks.each do |task| %>
				<div class="title3">
					- <%= task.name %>
				</div>
			<% end %>
	<% end %>
</div>



</div>

